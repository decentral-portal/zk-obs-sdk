{
  "compilerOptions": {
    "lib": [
      "es2020", "esnext"
    ],
    "declaration": true,
    "target": "esnext",
    "module": "commonjs",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "allowJs": true,
    "checkJs": false,
    "allowUnreachableCode": true,
    "outDir": "dist",
  },
  "include":[
    "lib/**/*.ts",
    "scripts/**/*.js",
    "test/**/*.ts",
    "circuits_src/**/*.ts"
  ]
}
